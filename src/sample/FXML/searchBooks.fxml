<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="130.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controllers.ControllerSearchBooks">
   <children>
      <ChoiceBox fx:id="searchBy" layoutX="24.0" layoutY="31.0" prefHeight="25.0" prefWidth="160.0" />
      <TextField fx:id="searchByTextField" layoutX="208.0" layoutY="31.0" prefHeight="25.0" prefWidth="211.0" />
      <Button fx:id="buttonSearch" layoutX="151.0" layoutY="77.0" mnemonicParsing="false" onAction="#search" text="Search">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="buttonCancel" layoutX="229.0" layoutY="77.0" mnemonicParsing="false" onAction="#cancel" text="Cancel">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
